<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="cn.githink.mine">

    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <application
            android:name="cn.githink.common.App"
            android:allowBackup="true"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:icon="@mipmap/ic_launcher"
            android:theme="@style/AppTheme"
            tools:ignore="GoogleAppIndexingWarning"
            android:screenOrientation="portrait">
        <activity android:name="cn.githink.mine.presenter.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="cn.githink.mine.presenter.activity.RegisterActivity"/>
    </application>

</manifest>
